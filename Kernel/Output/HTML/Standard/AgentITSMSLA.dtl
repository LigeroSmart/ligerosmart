<!-- dtl:block:Overview -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Overview"}: $Text{"SLA"}$Env{"Box1"}
    </td>
  </tr>
  <tr>
    <td class="mainbody" align="center">
      <br>
      <table border="0" width="800" cellspacing="0" cellpadding="3">
        <tr class="contenthead">
          <td class="contentkey" width="10">&nbsp;</td>
          <td class="contentkey">$Text{"SLA"}</td>
          <td class="contentkey">$Text{"Type"}</td>
          <td class="contentkey" width="150">$Text{"Changed"}</td>
        </tr>
<!-- dtl:block:OverviewRow -->
        <tr class="contentbody">
          <td class="$QData{"CssClass"}">&nbsp;</td>
          <td class="$QData{"CssClass"}">
            <a href="$Env{"Baselink"}Action=AgentITSMSLAZoom&SLAID=$QData{"SLAID"}">
            $QData{"Name"}
            </a>
          </td>
          <td class="$QData{"CssClass"}">$Text{"$QData{"Type"}"}</td>
          <td class="$QData{"CssClass"}" width="150">$TimeLong{"$QData{"ChangeTime"}"}</td>
        </tr>
<!-- dtl:block:OverviewRow -->
        <tr class="contentfooter">
          <td colspan="4">&nbsp;</td>
        </tr>
      </table>
      <br>
    </td>
  </tr>
</table>
<!-- dtl:block:Overview -->
