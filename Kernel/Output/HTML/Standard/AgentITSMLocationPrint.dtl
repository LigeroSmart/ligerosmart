<table border="0" cellspacing="1" cellpadding="0" width="100%">
  <tr>
    <td class="mainhead">$Text{"Location"}</td>
  </tr>
</table>
<table border="0" cellspacing="1" cellpadding="10" width="100%">
  <tr>
    <td class="contentkey">
      $Text{"Location-Info"}:
      <table border="0" cellspacing="3" cellpadding="0" width="100%">
        <tr>
          <td class="contentkey" width="10%">$Text{"Location"}:</td>
          <td class="contentvalue" width="40%">$QData{"NameShort"}</td>
          <td class="contentkey" width="20%">$Text{"Created"}:</td>
          <td class="contentvalue" width="30%">$TimeLong{"$Data{"CreateTime"}"}</td>
        </tr>
        <tr>
          <td class="contentkey"></td>
          <td class="contentvalue"></td>
          <td class="contentkey">$Text{"Created by"}:</td>
          <td class="contentvalue">$QData{"CreateByUserLogin"} ($QData{"CreateByUserFirstname"} $QData{"CreateByUserLastname"})</td>
        </tr>
        <tr>
          <td class="contentkey"></td>
          <td class="contentvalue"></td>
          <td class="contentkey">$Text{"Last changed"}:</td>
          <td class="contentvalue">$TimeLong{"$Data{"ChangeTime"}"}</td>
        </tr>
        <tr>
          <td class="contentkey"></td>
          <td class="contentvalue"></td>
          <td class="contentkey">$Text{"Last changed by"}:</td>
          <td class="contentvalue">$QData{"ChangeByUserLogin"} ($QData{"ChangeByUserFirstname"} $QData{"ChangeByUserLastname"})</td>
        </tr>
        <tr>
          <td class="contentkey">&nbsp;</td>
          <td colspan="3" class="contentvalue">&nbsp;</td>
        </tr>
      </table>
    </td>
  </tr>
</table>
<table border="0" width="640" cellpadding="0" cellspacing="0" bgcolor="#000000" class="container">
  <tr>
    <td>
      <table border="0" width="640" cellpadding="10" cellspacing="1" bgcolor="#ffffff">
        <tr>
          <td>
            <table border="0" width="100%" cellspacing="0" cellpadding="3">
              <tr bgcolor="#ffffff">
                <td valign="top" width="100%" class="small">
                  <table border="0" width="100%" cellspacing="0" cellpadding="1">
                    <tr>
                      <td class="contentkey" width="20%">$Text{"Location"}:</td>
                      <td class="contentvalue">$QData{"Name"}</td>
                    </tr>
                    <tr>
                      <td class="contentkey">$Text{"Type"}:</td>
                      <td class="contentvalue">$Text{"$QData{"Type"}"}</td>
                    </tr>
                    <tr>
                      <td class="contentkey">$Text{"Phone 1"}:</td>
                      <td class="contentvalue">$QData{"Phone1"}</td>
                    </tr>
                    <tr>
                      <td class="contentkey">$Text{"Phone 2"}:</td>
                      <td class="contentvalue">$QData{"Phone2"}</td>
                    </tr>
                    <tr>
                      <td class="contentkey">$Text{"Fax"}:</td>
                      <td class="contentvalue">$QData{"Fax"}</td>
                    </tr>
                    <tr>
                      <td class="contentkey">$Text{"E-Mail"}:</td>
                      <td class="contentvalue">$QData{"Email"}</td>
                    </tr>
                    <tr>
                      <td class="contentkey">$Text{"Address"}:</td>
                      <td class="contentvalue">$Data{"Address"}</td>
                    </tr>
                  </table>
                </td>
              </tr>
            </table>
          </td>
        </tr>
      </table>
    </td>
  </tr>
</table>