<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td class="mainhead">
            <div title="$Env{"Box0"} $Text{"Zoom"} $Config{"ITSMWorkOrder::Hook"}: $QData{"ChangeNumber"}-$QData{"WorkOrderNumber"} $Env{"Box1"} $QData{"WorkOrderTitle"}">
                $Env{"Box0"} $Text{"Zoom"} $Config{"ITSMWorkOrder::Hook"}: $QData{"ChangeNumber"}-$QData{"WorkOrderNumber"} $Env{"Box1"} $QData{"WorkOrderTitle","70"}
            </div>
        </td>
    </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <div class="ControlRow">
            <h2></h2>
        </div>
        <div class="ActionRow">
            <ul class="Actions">
<!-- dtl:block:Menu -->
<!-- dtl:block:MenuItem -->
                <li>
                    <a href="$Env{"Baselink"}$Data{"Link"}" class="$QData{"MenuClass"}" title="$Text{"$Data{"Description"}"}">$Text{"$Data{"Name"}"}</a>
                </li>
<!-- dtl:block:MenuItem -->
<!-- dtl:block:Menu -->
            </ul>
        </div>
        <td align="right" width="25%" class="menu"></td>
    </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <colgroup>
        <col width="75%">
        <col width="25%">
    </colgroup>
    <tr>
        <td valign="top" class="mainbody">
            <table border="0" width="100%" cellspacing="0" cellpadding="1">
                <colgroup>
                    <col width="30">
                    <col>
                </colgroup>
                <tr>
                    <td style="text-align:center;">
                        <img border="0" src="$Config{"Frontend::ImagePath"}$QData{"ChangeStateSignal"}.png" title="$Text{"$Data{"ChangeState"}"}" alt="$Text{"$Data{"ChangeState"}"}">
                    </td>
                    <td>
                        <span style="font-weight:bold;">
                            $Config{"ITSMChange::Hook"} $QData{"ChangeNumber"}
