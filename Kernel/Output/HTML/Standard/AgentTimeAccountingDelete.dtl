<fieldset class="TableLike W300px">
    <label>$Text{"Year"}:</label>
    <p class="Value">
        $QData{"Year"}
    </p>
    <div class="Clear"></div>

    <label>$Text{"Month"}:</label>
    <p class="Value">
        $Text{"$Data{"Month"}"}
    </p>
    <div class="Clear"></div>

    <label>$Text{"Day"}:</label>
    <p class="Value">
        $QData{"Day"}
    </p>
    <div class="Clear"></div>
</fieldset>
<p class="SpacingTop Center">
    $Text{"Do you really want to delete the Time Accounting of this day?"}
</p>
