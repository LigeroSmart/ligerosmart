<div class="WidgetPopup ARIARoleMain">

    <div class="Header">
        <h1>
            $Text{"History of"} $Config{"ITSMChange::Hook"}: $QData{"ChangeNumber"} - $QData{"ChangeTitle","60"}
        </h1>
        <p>
            <a href="#" class="CancelClosePopup">$Text{"Cancel & close window"}</a>
        </p>
    </div>

    <div class="Content">
        <div class="WidgetSimple">
            <div class="Header">
                <h2>
                    $Text{"History Content"}
                </h2>
            </div>
            <div class="Content">
                <table class="DataTable">
                    <thead>
                        <tr>
                            <th>$Text{"Action"}</th>
                            <th>$Text{"Comment"}</th>
                            <th>$Text{"Details"}</th>
                            <th>$Text{"Workorder"}</th>
                            <th>$Text{"User"}</th>
                            <th>$Text{"Createtime"}</th>
                        </tr>
                    </thead>
                    <tbody>

<!-- dtl:block:Row -->
                        <tr>
                            <td>
                                $QData{"HistoryType"}
                            </td>
                            <td>
                                $QData{"Content","100"}
                            </td>
                            <td>
<!-- dtl:block:NoHistoryZoom -->
                                -
<!-- dtl:block:NoHistoryZoom -->
<!-- dtl:block:ShowHistoryZoom -->
                                <a class="LinkZoomView AsBlock" href="$Env{"Baselink"}Action=AgentITSM$Data{"ZoomType"}HistoryZoom;HistoryEntryID=$Data{"HistoryEntryID"}">$Text{"Zoom view"}</a>
<!-- dtl:block:ShowHistoryZoom -->
                            </td>
                            <td>
<!-- dtl:block:NoWorkOrderZoom -->
                                -
<!-- dtl:block:NoWorkOrderZoom -->
<!-- dtl:block:ShowWorkOrderZoom -->
                                <a class="LinkZoomView AsBlock" href="$Env{"Baselink"}Action=AgentITSMWorkOrderZoom;WorkOrderID=$Data{"WorkOrderID"}">$Text{"Zoom view"}</a>
<!-- dtl:block:ShowWorkOrderZoom -->
                            </td>
                            <td>
                                $QData{"UserLogin"} ($Quote{"$Data{"UserFirstname"} $Data{"UserLastname"}","20"})
                            </td>
                            <td>
                                $TimeLong{"$Data{"CreateTime"}"}
                            </td>
                        </tr>
<!-- dtl:block:Row -->

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>