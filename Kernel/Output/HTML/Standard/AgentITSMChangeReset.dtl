<div class="LayoutPopup ARIARoleMain">

    <div class="Header">
        <h1>$Text{"Reset"} $Config{"ITSMChange::Hook"}: $QData{"ChangeNumber"} &ndash; $QData{"ChangeTitle"}</h1>
        <p>
            <a href="#" class="CancelClosePopup">$Text{"Cancel & close window"}</a>
        </p>
    </div>

    <div class="Content">

        <div class="Content">
            <p>
                $Text{"By clicking 'Confirm Change Reset', the Change and all its workorders will be reset."}
            </p>

            <form action="$Env{"CGIHandle"}" method="post" name="compose" id="ChangeEditForm" enctype="multipart/form-data" class="Validate PreventMultipleSubmits">
                <input type="hidden" name="Action"    value="$Env{"Action"}"/>
                <input type="hidden" name="Subaction" value="Reset"/>
                <input type="hidden" name="ChangeID"  value="$QData{"ChangeID"}"/>

                <div class="Field SpacingTop">
                    <button class="Primary" id="SubmitChangeReset" name="ChangeReset" title="$Text{"Confirm Change Reset"}" type="submit" value="$Text{"Confirm Change Reset"}">$Text{"Confirm Change Reset"}</button>
                </div>
            </form>

        </div>
    </div>
</div>
