<!-- dtl:block:Overview -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Overview"}: $Text{"Service"}$Env{"Box1"}
    </td>
  </tr>
  <tr>
    <td class="mainbody" align="center">
      <br>
      <table border="0" width="800" cellspacing="0" cellpadding="3">
        <tr class="contenthead">
          <td class="contentkey" width="20">&nbsp;</td>
          <td class="contentkey">$Text{"Service"}</td>
          <td class="contentkey">$Text{"Type"}</td>
          <td class="contentkey">$Text{"Criticality"}</td>
          <td class="contentkey" width="150">$Text{"Changed"}</td>
        </tr>
<!-- dtl:block:OverviewRow -->
        <tr class="contentbody">
          <td class="$QData{"CssClass"}" align="right">
            <img border="0" src="$Env{"Images"}$QData{"CurInciSignal"}.png" title="$Quote{"$Text{"$Data{"CurInciState"}"}"}" alt="$Quote{"$Text{"$Data{"CurInciState"}"}"}">
          </td>
          <td class="$QData{"CssClass"}">
<!-- dtl:block:OverviewRowSpace -->
            &nbsp;&nbsp;&nbsp;&nbsp;
<!-- dtl:block:OverviewRowSpace -->
            <a href="$Env{"Baselink"}Action=AgentITSMServiceZoom&ServiceID=$QData{"ServiceID"}">
            $QData{"Name"}
            </a>
          </td>
          <td class="$QData{"CssClass"}">$Text{"$QData{"Type"}"}</td>
          <td class="$QData{"CssClass"}">$Text{"$QData{"Criticality"}"}</td>
          <td class="$QData{"CssClass"}" width="150">$TimeLong{"$QData{"ChangeTime"}"}</td>
        </tr>
<!-- dtl:block:OverviewRow -->
        <tr class="contentfooter">
          <td colspan="5">&nbsp;</td>
        </tr>
      </table>
      <br>
    </td>
  </tr>
</table>
<!-- dtl:block:Overview -->
