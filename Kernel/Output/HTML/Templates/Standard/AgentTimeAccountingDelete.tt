<fieldset class="TableLike W300px">
    <label>[% Translate("Year") | html %]:</label>
    <p class="Value">
        [% Data.Year | html %]
    </p>
    <div class="Clear"></div>

    <label>[% Translate("Month") | html %]:</label>
    <p class="Value">
        [% Translate(Data.Month) | html %]
    </p>
    <div class="Clear"></div>

    <label>[% Translate("Day") | html %]:</label>
    <p class="Value">
        [% Data.Day | html %]
    </p>
    <div class="Clear"></div>
</fieldset>
<p class="SpacingTop Center">
    [% Translate("Do you really want to delete the Time Accounting of this day?") | html %]
</p>
