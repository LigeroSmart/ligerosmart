<div id="MainBox" class="TicketView ARIARoleMain 0 " role="main">
    <h1 class="InvisibleText">Visão Geral do catálogo de serviços</h1>
[% RenderBlockStart("ConfigItemHeader") %]
        <div class="Headline">
            <div class="Flag" title="[% Translate(Data.CurInciState) | html %]">
                <span class="[% Data.CurInciSignal | html %]"></span>
            </div>
            <h1  title="[% Translate("QRCode item") | html %]: [% Data.Number | html %] &ndash; [% Data.Name | truncate(40) | html %]">
                [% Translate("QRCode item") | html %]: [% Data.Number | html %] <span>&mdash;</span> [% Data.Name | truncate(60) | html %]
            </h1>    
        </div>
[% RenderBlockEnd("ConfigItemHeader") %]    
    <ul id="ulBreadCrumb" class="BreadCrumb">
[% RenderBlockStart("BreadcrumbStart") %]
        <li>
            <a href="[% Env("Baselink") %]Action=[%Data.Action | html %]" title="Retornar para a visão geral de serviços">
                <i class="fa fa-home"></i>
            </a>
        </li>
[% RenderBlockEnd("BreadcrumbStart") %]
[% RenderBlockStart("BreadcrumbServices") %]
        <li>
            <a href="[%Data.BreadcrumbLink | html %]" title=[%Data.BreadcrumbTitle | html %]>
                [%Data.BreadcrumbTitle | html %]
            </a>
        </li>
[% RenderBlockEnd("BreadcrumbServices") %]       
    </ul>
<script type="text/javascript">
    const ulBreadCrumb = document.getElementById("ulBreadCrumb");
    const ulBreadCrumblength = document.querySelectorAll("#ulBreadCrumb li").length;
    if (!ulBreadCrumblength) {
        ulBreadCrumb.style.display = "none";
    }
</script>
<div class="Content">
<div class="LayoutFixedSidebar SidebarFirst">    
    <div class="ContentColumn">
            <div class="WidgetSimpleAgent">
                <div class="Header">
                    <h2>
                        [% Translate("What are you looking for?") %]
                    </h2>
                </div>
                <div class="Content">
                    <span class="TableFilterContainer">
[% RenderBlockStart("FirstLvl") %]
                        <input type="text" id="LigeroServiceSearch" autocomplete="off" class="W95pc" name="Filter" value="" title="Filtro" placeholder="Basta começar a digitar para filtrar... ">
[% RenderBlockEnd("FirstLvl") %]
[% RenderBlockStart("CategorySearch") %]
                        <input type="text" id="LigeroServiceSearchMark" autocomplete="off" class="W95pc" name="Filter" value="" title="Filtro" placeholder="Basta começar a digitar para filtrar... ">
[% RenderBlockEnd("CategorySearch") %]                
                    </span>
                </div>
            </div>	
        <input type="hidden" id="StartHit" value='0'>
[% RenderBlockStart("ConfigItemID") %]
        <input type="hidden" id="ConfigItemID" value='[% Data.ConfigItemID %]'>
[% RenderBlockEnd("ConfigItemID") %]
[% RenderBlockStart("CategoryFaqDesc") %]
        <h2 class="CategoryFaqDescTitle">[% Data.FaqTitle %]</h1>
        <div class="CategoryFaqDescText">[% Data.FaqDescription %]</div>
[% RenderBlockEnd("CategoryFaqDesc") %]
        <div id="ServiceCatalog">
            <div id="ServiceCatalogItens"> 
                <div class="WidgetSimpleAgent">                
[% RenderBlockStart("CategoryTitle") %]
                    <div class="Header">
                        <h2>[% Data.CategoryTitle %]</h2>
                    </div>
[% RenderBlockEnd("CategoryTitle") %]
				<div id="divwhatdoyouneed" class="WidgetSimpleAgent" style="">			                               
[% RenderBlockStart("NeedMessage") %]
                    <div class="Header">
                        <h2>[% Translate("What do you need?") | html %]</h2>
                    </div>
[% RenderBlockEnd("NeedMessage") %]					
					<div class="Content">
						<table id="tablewhatdoyouneed" class="Overview" style=" ">
							<tbody>
[% RenderBlockStart("LayoutActivity") %]
								<tr class="MasterAction">
                                    <td class="Title">
                                        <a href="[% Env("Baselink") %]Action=[%Data.Action | html %]&Subaction=NextLevel&KeyPrimary=[%Data.KeyPrimary | html %]" class="MasterActionLink">
                                            <div>
                                                <h2>[%Data.LayoutServiceName %]</h2>
                                                &nbsp;-&nbsp; [% Data.LayoutServiceDescription %]
                                            </div>
                                        </a>
                                    </td>
								</tr> 
[% RenderBlockEnd("LayoutActivity") %]
[% RenderBlockStart("LayoutActivityUrl") %]
								<tr class="MasterAction">
                                    <td class="Title">
                                        <a target="[% Data.UrlTarget | html %]" href="[% Data.Url | html %]" class="MasterActionLink">
                                            <div>
                                                <h2>[%Data.LayoutServiceName %]</h2>
                                                &nbsp;-&nbsp; [% Data.LayoutServiceDescription %]
                                            </div>
                                        </a>
                                    </td>
								</tr>
[% RenderBlockEnd("LayoutActivityUrl") %]
[% RenderBlockStart("LayoutServiceArticle") %]
								<tr class="MasterAction">
                                    <td class="Title">
                                        <a target="[% Data.UrlTarget | html %]" href="[% Data.LayoutServiceLink| html %]" class="MasterActionLink">
                                            <div>
                                                <h2>[%Data.LayoutServiceName %]</h2>
                                                &nbsp;-&nbsp; [% Data.LayoutServiceDescription %]
                                            </div>
                                        </a>
                                    </td>
								</tr>
[% RenderBlockEnd("LayoutServiceArticle") %]                
							</tbody>
                    	</table>
<script>
    const tablewhatdoyouneed = document.getElementById("tablewhatdoyouneed");
    const divwhatdoyouneed = document.getElementById("divwhatdoyouneed");
    const rows = tablewhatdoyouneed.rows;
    if (!rows.length) {
        divwhatdoyouneed.style.display = "none";
    }
</script>                        
					</div>
                </div>
             
[% RenderBlockStart("MoreServices") %]
                        <div class="Header">
                            <h2>[% Translate("More Services:") | html %]</h2>
                        </div>
[% RenderBlockEnd("MoreServices") %]
