                                    <span class="Icons">
                            			<i class="fa [%Data.Icons | html %]"></i>
                                    </span>
        		[% CASE %]
                                    <span class="Icons" style="background:url([%Data.LayoutServiceLink | html %]) no-repeat 50% 50%;
                                    background-size: 20px 20px; background-color:[% Data.Color %];"></span>       
       		[% END %]
                        			
                        			<span class="InvisibleText">[%Data.LayoutServiceName %]</span>
                        			<span class="Title">
                            			[%Data.LayoutServiceName %]
                            			<span class="InvisibleText">[%Data.LayoutServiceName %]</span>
                        			</span>                             
                        			<span class="Description">
                            			[%Data.LayoutServiceDescription %]
                            			<span class="InvisibleText">[%Data.LayoutServiceDescription %]</span>
                        			</span>
                    			</a>
                    		</li>
        [% CASE '2' %]
                    		<li class=" ">            
                    			<a class="AsBlock" href="[% Env("Baselink") %]Action=[%Data.Action | html %]&Subaction=NextLevel&KeyPrimary=[%Data.KeyPrimary | html %]">                        
                        			
        	[% SWITCH Data.LayoutServiceLink %]
        		[% CASE '#' %]
                                    <span class="Icons">
                            			<i class="fa [%Data.Icons | html %]"></i>
                                    </span>
        		[% CASE %]
                                    <span class="Icons" style="background:url([%Data.LayoutServiceLink | html %]) no-repeat 50% 50%;
                                    background-size: 20px 20px; background-color:[% Data.Color %];"></span>        
       		[% END %]
                        			<span class="InvisibleText">[%Data.LayoutServiceName %]</span>
                        			<span class="Title">
                            			[%Data.LayoutServiceName %]
                            			<span class="InvisibleText">[%Data.LayoutServiceName %]</span>
                        			</span>                             
                        			<span class="Description">
                            			[%Data.LayoutServiceDescription %]
                            			<span class="InvisibleText">[%Data.LayoutServiceDescription %]</span>
                        			</span>
                    			</a>
                    		</li>
        [% CASE '3' %]
							<li class=" "> 					            
                    			<a class="AsBlock" href="[% Env("Baselink") %]Action=[%Data.Action | html %]&Subaction=NextLevel&KeyPrimary=[%Data.KeyPrimary | html %]">                        
                        			
        	[% SWITCH Data.LayoutServiceLink %]
