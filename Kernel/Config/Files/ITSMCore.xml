    <ConfigItem Name="Frontend::Module###AdminTicketPriority" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for the AdminTicketPriority configuration in the admin area.</Description>
        <Description Lang="de">Frontendmodul-Registration der AdminTicketPriority Konfiguration im Admin-Bereich.</Description>
        <Group>Ticket</Group>
        <SubGroup>Frontend::Admin::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <Group>admin</Group>
                <Description>Admin</Description>
                <Title>Priority</Title>
                <NavBarName>Admin</NavBarName>
                <NavBarModule>
                    <Module>Kernel::Output::HTML::NavBarModuleAdmin</Module>
                    <Name>Priority</Name>
                    <Block>Block3</Block>
                    <Prio>420</Prio>
                </NavBarModule>
            </FrontendModuleReg>
        </Setting>
