<?xml version="1.0" encoding="iso-8859-1" ?>
<otrs_config version="1.0" init="Config">
    <CVS>$Id: ITSMChangeManagement.xml,v 1.8 2009-10-19 19:58:23 mae Exp $</CVS>
    <ConfigItem Name="ITSMChange::CounterLog" Required="1" Valid="1">
        <Description Lang="en">Logfile for the itsm change counter.</Description>
        <Description Lang="de">Pfad und Verzeichnis zur Logdatei für den ITSM Change-Counter.</Description>
        <Group>ITSM Change Management</Group>
        <SubGroup>Core::ITSMChange</SubGroup>
        <Setting>
            <String Regex="">&lt;OTRS_CONFIG_Home&gt;/var/log/ITSMChangeCounter.log</String>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Frontend::Module###AgentITSMChange" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for the AgentITSMChange object in the agent interface.</Description>
        <Description Lang="de">Frontendmodul-Registration des AgentITSMChange-Objekts im Agent-Interface.</Description>
        <Group>ITSM Change Management</Group>
        <SubGroup>Frontend::Agent::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <GroupRo>itsm-change</GroupRo>
                <Description>ITSM Change Overview</Description>
                <NavBarName>Change</NavBarName>
                <Title>Change</Title>
                <NavBar>
                    <Description>Change-Area</Description>
                    <Name>Change</Name>
                    <Type>Menu</Type>
                    <Block>ItemArea</Block>
                    <Image>itsm_change.png</Image>
                    <Link>Action=AgentITSMChange</Link>
                    <NavBar>Change</NavBar>
                    <AccessKey></AccessKey>
                    <Prio>3400</Prio>
                </NavBar>
                <NavBar>
                    <Description>Overview</Description>
                    <Name>Overview</Name>
                    <Image>overview.png</Image>
                    <Link>Action=AgentITSMChange</Link>
                    <Type></Type>
                    <Block></Block>
                    <NavBar>Change</NavBar>
                    <AccessKey></AccessKey>
                    <Prio>100</Prio>
                </NavBar>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Frontend::Module###AgentITSMChangeZoom" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for the AgentITSMChangeZoom object in the agent interface.</Description>
        <Description Lang="de">Frontendmodul-Registration des AgentITSMChangeZoom-Objekts im Agent-Interface.</Description>
        <Group>ITSM Configuration Management</Group>
        <SubGroup>Frontend::Agent::ModuleRegistration</SubGroup>
