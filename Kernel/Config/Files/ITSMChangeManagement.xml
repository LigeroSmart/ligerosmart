    <ConfigItem Name="ITSMChange::Frontend::MenuModule###000-Back" Required="0" Valid="1">
        <Description Lang="en">Module to show back link in change menu.</Description>
        <Description Lang="de">Über dieses Modul wird der Zurück-Link in der Linkleiste der Change-Ansicht angezeigt.</Description>
        <Group>ITSM Change Management</Group>
        <SubGroup>Frontend::Agent::ITSMChange::MenuModule</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::ITSMChangeMenuGeneric</Item>
                <Item Key="Name">Back</Item>
                <Item Key="Description">Back</Item>
                <Item Key="Action"></Item>
                <Item Key="Link">Action=AgentITSMChange&amp;ChangeID=$QData{"ChangeID"}</Item>
                <Item Key="LinkParam">onclick="javascript:history.back(); return false;"</Item>
            </Hash>
        </Setting>
    </ConfigItem>
