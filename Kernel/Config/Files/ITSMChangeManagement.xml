        <Setting>
            <FrontendModuleReg>
                <GroupRo>itsm-change</GroupRo>
                <Description>Search Changes</Description>
                <Title>Search</Title>
                <NavBarName>Change</NavBarName>
                <NavBar>
                    <Description>Search Changes</Description>
                    <Name>Search</Name>
                    <Image>search.png</Image>
                    <Link>Action=AgentITSMChangeSearch</Link>
                    <NavBar>Change</NavBar>
