                <Description>Change Zoom</Description>
                <Title>Zoom</Title>
                <NavBarName>Change</NavBarName>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Frontend::Module###AgentITSMChangeAdd" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for the AgentITSMChangeAdd object in the agent interface.</Description>
        <Description Lang="de">Frontendmodul-Registration des AgentITSMChangeAdd-Objekts im Agent-Interface.</Description>
        <Group>ITSM Change Management</Group>
        <SubGroup>Frontend::Agent::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <Group>itsm-change</Group>
                <Description>Change Add</Description>
                <Title>Add</Title>
                <NavBarName>Change</NavBarName>
                <NavBar>
                    <Description>New</Description>
                    <Name>New</Name>
                    <Image>new.png</Image>
                    <Link>Action=AgentITSMChangeAdd</Link>
                    <Type></Type>
                    <Block></Block>
                    <NavBar>Change</NavBar>
                    <AccessKey></AccessKey>
                    <Prio>200</Prio>
                </NavBar>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Frontend::Module###AgentITSMChangeSearch" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for the AgentITSMChangeSearch object in the agent interface.</Description>
        <Description Lang="de">Frontendmodul-Registration des AgentITSMChangeSearch-Objekts im Agent-Interface.</Description>
        <Group>ITSM Change Management</Group>
