<?xml version="1.0" encoding="iso-8859-1" ?>
<otrs_config version="1.0" init="Framework">
    <CVS>$Id: ImportExport.xml,v 1.3 2008-01-24 16:33:56 mh Exp $</CVS>
    <ConfigItem Name="Frontend::Module###AdminImportExport" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for the AdminImportExport in the admin area.</Description>
        <Description Lang="de">Frontendmodul-Registration der AdminImportExport im Admin-Bereich.</Description>
        <Group>ImportExport</Group>
        <SubGroup>Frontend::Admin::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <Group>admin</Group>
                <Description>Admin</Description>
                <Title>Import/Export</Title>
                <NavBarName>Admin</NavBarName>
                <NavBarModule>
                    <Module>Kernel::Output::HTML::NavBarModuleAdmin</Module>
                    <Name>Import/Export</Name>
                    <Block>Block4</Block>
                    <Prio>710</Prio>
                </NavBarModule>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="ImportExport::FormatBackendRegistration###CSV" Required="0" Valid="1">
        <Description Lang="en">Format backend module registration for the import/export modul.</Description>
        <Description Lang="de">Format-Backend Modul Registration des Import/Export Moduls.</Description>
        <Group>ImportExport</Group>
        <SubGroup>FormatBackend::ModuleRegistration</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::System::ImportExport::FormatBackend::CSV</Item>
                <Item Key="Name">CSV</Item>
            </Hash>
        </Setting>
    </ConfigItem>
</otrs_config>